# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URLs
MANTLE_RPC_URL=https://rpc.mantle.xyz
MANTLE_TESTNET_RPC_URL=https://rpc.testnet.mantle.xyz

# Etherscan API keys (optional, for contract verification)
MANTLE_ETHERSCAN_API_KEY=your_mantle_etherscan_api_key
MANTLE_TESTNET_ETHERSCAN_API_KEY=your_mantle_testnet_etherscan_api_key

# Gas settings (optional)
GAS_LIMIT=3000000
GAS_PRICE=20000000000
